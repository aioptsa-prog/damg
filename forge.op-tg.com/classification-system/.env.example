APP_ENV=local
APP_DEBUG=true
APP_KEY=your_app_key_here

DB_CONNECTION=sqlite
DB_DATABASE=storage/database.sqlite

SEEDER_SERVICE_ENABLED=true
TYPEAHEAD_SERVICE_ENABLED=true
ICON_SERVICE_ENABLED=true

# Optional settings
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_DRIVER=sync

# Security settings
CSRF_TOKEN_SECRET=your_csrf_token_secret_here
HMAC_SECRET=your_hmac_secret_here

# Other configurations
LOG_CHANNEL=stack
LOG_LEVEL=debug